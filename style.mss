#waterps [Level_='watch']{
  line-color:#435f99;
  line-width:0.5;
  polygon-opacity:0.6;
  polygon-fill:#436f15;
}

#waterps [Level_='vulnerable']{
  line-color:#435f99;
  line-width:0.5;
  polygon-opacity:0.6;
  polygon-fill:#ddbe20;
}

#waterps [Level_='critical']{
  line-color:#435f99;
  line-width:0.5;
  polygon-opacity:0.6;
  polygon-fill:#ff0000;
}

#waterps [Level_='pending']{
  line-color:#435f99;
  line-width:0.5;
  polygon-opacity:0.4;
  polygon-fill:#788678;
}

#waterps [zoom > 12]{
  text-name: [PWS_Name];
  text-face-name: "Verdana Regular";
  text-halo-fill: #fff;
  text-halo-radius:1;
}